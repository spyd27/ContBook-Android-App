<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Username"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.contbook.spy_d.contbookdemo.SearchActivity">

    <TextView
        android:id="@+id/TextViewLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="12dp"
        android:text="@string/log_out" />

    <EditText
        android:id="@+id/editTextSearch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:ems="10"
        android:hint="Enter username to get info"
        android:inputType="textPersonName"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/tName"
        android:layout_alignStart="@+id/tName" />

    <Button
        android:id="@+id/buttonSearch"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/editTextSearch"
        android:layout_marginEnd="22dp"
        android:layout_marginRight="22dp"
        android:background="@color/colorPrimary"
        android:text="Search"
        android:textColor="@android:color/background_light" />

    <TextView
        android:id="@+id/textViewName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textViewUserEmail"
        android:layout_alignStart="@+id/textViewUserEmail"
        android:layout_below="@+id/buttonSearch"
        android:layout_marginTop="56dp" />

    <TextView
        android:id="@+id/textViewUserEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textViewCont"
        android:layout_alignStart="@+id/textViewCont"
        android:layout_below="@+id/textViewName"
        android:layout_marginTop="22dp" />

    <TextView
        android:id="@+id/textViewCont"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textViewFb"
        android:layout_alignStart="@+id/textViewFb"
        android:layout_below="@+id/textViewUserEmail"
        android:layout_marginTop="24dp" />

    <TextView
        android:id="@+id/textViewFb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textViewLn"
        android:layout_alignStart="@+id/textViewLn"
        android:layout_below="@+id/textViewCont"
        android:layout_marginTop="23dp" />

    <TextView
        android:id="@+id/textViewLn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textViewGit"
        android:layout_alignStart="@+id/textViewGit"
        android:layout_below="@+id/textViewFb"
        android:layout_marginTop="23dp" />

    <TextView
        android:id="@+id/textViewGit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextViewLogout"
        android:layout_alignStart="@+id/TextViewLogout"
        android:layout_below="@+id/textViewLn"
        android:layout_marginTop="37dp" />

    <TextView
        android:id="@+id/tName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textViewName"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp" />

    <TextView
        android:id="@+id/tEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewUserEmail"
        android:layout_alignBottom="@+id/textViewUserEmail"
        android:layout_alignLeft="@+id/tName"
        android:layout_alignStart="@+id/tName" />

    <TextView
        android:id="@+id/tCont"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewCont"
        android:layout_alignBottom="@+id/textViewCont"
        android:layout_alignLeft="@+id/tEmail"
        android:layout_alignStart="@+id/tEmail" />

    <TextView
        android:id="@+id/tFb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewFb"
        android:layout_alignBottom="@+id/textViewFb"
        android:layout_alignLeft="@+id/tCont"
        android:layout_alignStart="@+id/tCont" />

    <TextView
        android:id="@+id/tLn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textViewGit"
        android:layout_alignLeft="@+id/tFb"
        android:layout_alignStart="@+id/tFb" />

    <TextView
        android:id="@+id/tGit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textViewGit"
        android:layout_alignBottom="@+id/textViewGit"
        android:layout_alignLeft="@+id/tLn"
        android:layout_alignStart="@+id/tLn" />




</RelativeLayout>
